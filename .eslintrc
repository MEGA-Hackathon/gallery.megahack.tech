{
    "parserOptions": {
    "jsx": true,
    "useJSXTextNode": true,
    "ecmaVersion": 2020,
    "sourceType": "module",
    },
    "env": {"browser": true, "node": true},
    "ignorePatterns": [
    "/dist"
    ],
    "plugins": [
        "prettier"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:prettier/recommended"
    ],
    "rules": {
    "prettier/prettier": "warn"
    }
}